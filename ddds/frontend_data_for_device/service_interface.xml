<?xml version="1.0" encoding="utf-8"?>
<service_interface>

  <action name="ReportStartSession">
    <parameters>
      <parameter predicate="start_session_mock_data" format="value"/>
    </parameters>
    <failure_reasons/>
    <target>
      <device_module device="FrontendDataDevice"/>
    </target>
  </action>

  <query name="start_session_mock_data">
    <parameters/>
    <target>
      <device_module device="FrontendDataDevice"/>
    </target>
  </query>

  <query name="caller_phone_number">
    <parameters/>
    <target>
      <device_module device="FrontendDataDevice"/>
    </target>
  </query>

  <action name="MockPerformReport">
    <parameters/>
    <failure_reasons>
      <failure_reason name="no_mock_data"/>
    </failure_reasons>
    <target>
      <device_module device="FrontendDataDevice"/>
    </target>
  </action>

  <action name="GetMockEvent">
    <parameters/>
    <failure_reasons/>
    <target>
      <device_module device="FrontendDataDevice"/>
    </target>
  </action>

  <query name="phone_number_of_contact">
    <parameters>
      <parameter predicate="selected_contact" format="grammar_entry"/>
    </parameters>
    <target>
      <device_module device="FrontendDataDevice"/>
    </target>
  </query>

  <entity_recognizer name="ContactRecognizer">
    <target>
      <device_module device="FrontendDataDevice"/>
    </target>
  </entity_recognizer>

  <validator name="FrontendSessionDataAvailable">
    <parameters>
      <parameter predicate="selected_contact" format="grammar_entry"/>
    </parameters>
    <target>
      <device_module device="FrontendDataDevice"/>
    </target>
  </validator>

</service_interface>
