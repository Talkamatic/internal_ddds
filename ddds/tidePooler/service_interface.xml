<?xml version="1.0" encoding="utf-8"?>
<service_interface>
  <action name="OneshotTideIntent">
    <parameters>
      <parameter predicate="selected_city" format="value"/>
      <parameter predicate="Date" format="value"/>
      <parameter predicate="readable_selected_date" format="value"/>
      <parameter predicate="time_of_first_high_tide" format="value"/>
      <parameter predicate="height_of_first_high_tide" format="value"/>
    </parameters>
    <failure_reasons/>
    <target>
      <device_module device="TidepoolerDevice"/>
    </target>
  </action>
  <action name="SupportedCitiesIntent">
    <parameters>
      <parameter predicate="supported_cities" format="value"/>
    </parameters>
    <failure_reasons/>
    <target>
      <device_module device="TidepoolerDevice"/>
    </target>
  </action>
  <query name="supported_cities">
    <parameters/>
    <target>
      <device_module device="TidepoolerDevice"/>
    </target>
  </query>
  <query name="height_of_first_high_tide">
    <parameters/>
    <target>
      <device_module device="TidepoolerDevice"/>
    </target>
  </query>
  <query name="readable_selected_date">
    <parameters>
      <parameter predicate="Date" format="value"/>
    </parameters>
    <target>
      <device_module device="TidepoolerDevice"/>
    </target>
  </query>
  <query name="time_of_first_high_tide">
    <parameters/>
    <target>
      <device_module device="TidepoolerDevice"/>
    </target>
  </query>
  <entity_recognizer name="TidepoolerRecognizer">
    <target>
      <device_module device="TidepoolerDevice"/>
    </target>
  </entity_recognizer>
</service_interface>