<?xml version="1.0" encoding="utf-8"?>
<domain name="TidepoolerDomain" is_super_domain="true">

  <goal type="perform" action="top">
    <plan>
      <forget_all/>
      <findout type="alt_question">
        <alt>
          <perform action="OneshotTideIntent"/>
        </alt>
      </findout>
    </plan>
  </goal>


  <goal type="perform" action="OneshotTideIntent">
    <plan>
      <findout predicate="selected_city" type="wh_question"/>
      <findout predicate="Date" type="wh_question"/>
      <invoke_service_query predicate="readable_selected_date" type="wh_question"/>
      <invoke_service_query predicate="time_of_first_high_tide" type="wh_question"/>
      <invoke_service_query predicate="height_of_first_high_tide" type="wh_question"/>
      <invoke_service_action name="OneshotTideIntent" postconfirm="true"/>
    </plan>
  </goal>


  <goal type="perform" action="SupportedCitiesIntent">
    <plan>
      <invoke_service_query predicate="supported_cities" type="wh_question"/>
      <invoke_service_action name="SupportedCitiesIntent" postconfirm="true"/>
    </plan>
  </goal>

</domain>
