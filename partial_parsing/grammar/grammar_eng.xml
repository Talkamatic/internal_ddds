<?xml version="1.0" encoding="utf-8"?>
<grammar>
  <action name="navigate">
    <verb-phrase>
      <verb ref="navigate"/>
    </verb-phrase>
  </action>

  <question speaker="system" predicate="destination" type="wh_question">
    <utterance>what is your destination</utterance>
  </question>

  <report action="StartNavigation" status="ended">
      <utterance>started navigation</utterance>
  </report>

  <lexicon>
    <verb id="navigate">
      <infinitive>navigate</infinitive>
    </verb>
  </lexicon>
</grammar>
