<?xml version="1.0" encoding="utf-8"?>
<grammar>
    <action name="top">main menu</action>
    <action name="up">go back</action>
    <action name="navigate">
        <one-of>
            <item>navigate</item>
            <item>navigate to <slot predicate="destination"/></item>
        </one-of>
    </action>
    <report action="Navigate" status="ended">Destination set to <slot predicate="destination"/>.</report>
    <question speaker="system" predicate="destination" type="wh_question">what is your destination</question>
    <individual name="city_paris">Paris</individual>
</grammar>
