<?xml version="1.0" encoding="utf-8"?>
<service_interface>
  <play_audio_action name="ListenToArtist">
    <audio_url_parameter predicate="url_to_play"/>
    <parameters/>
    <target>
      <frontend/>
    </target>
  </play_audio_action>
  <action name="MockAction">
    <parameters>
      <parameter predicate="common_predicate" format="value"/>
    </parameters>
    <failure_reasons/>
    <target>
      <device_module device="MusicPlayerDevice"/>
    </target>
  </action>
  <action name="SetVolume">
    <parameters>
      <parameter predicate="volume" format="value"/>
    </parameters>
    <failure_reasons/>
    <target>
      <device_module device="MusicPlayerDevice"/>
    </target>
  </action>
  <query name="url_to_play">
    <parameters/>
    <target>
      <device_module device="MusicPlayerDevice"/>
    </target>
  </query>
  <entity_recognizer name="MockEntityRecognizer">
    <target>
      <device_module device="MusicPlayerDevice"/>
    </target>
  </entity_recognizer>
</service_interface>