<?xml version="1.0" encoding="utf-8"?>
<domain name="OutputVariationDomain" is_super_domain="true">
  <goal type="perform" action="top">
    <plan>
      <forget_all/>
      <findout type="goal"/>
    </plan>
  </goal>

  <goal type="perform" action="call">
    <plan>
      <findout type="wh_question" predicate="contact_to_call"/>
      <findout type="wh_question" predicate="number_type_to_call"/>
      <invoke_service_action name="Call" postconfirm="true"/>
    </plan>
  </goal>

  <goal type="perform" action="book_travel">
    <plan>
      <raise type="wh_question" predicate="departure_time"/>
      <findout type="wh_question" predicate="destination_city"/>
      <invoke_service_action name="BookTravel" postconfirm="true"/>
    </plan>
  </goal>

  <goal type="perform" action="play_radio">
    <plan>
      <raise type="wh_question" predicate="channel_to_play"/>
      <raise type="wh_question" predicate="category_to_play"/>
      <invoke_service_action name="PlayRadio" postconfirm="true"/>
    </plan>
  </goal>

  <goal type="resolve" question_type="wh_question" predicate="queried_phonenumber">
    <plan>
      <findout type="wh_question" predicate="queried_contact"/>
      <invoke_service_query type="wh_question" predicate="queried_phonenumber"/>
    </plan>
  </goal>

  <parameters question_type="wh_question" predicate="queried_contact">
    <ask_feature predicate="queried_contact_firstname"/>
    <ask_feature predicate="queried_contact_lastname"/>
  </parameters>
  <parameters question_type="wh_question" predicate="queried_contact_lastname">
    <background predicate="queried_contact_firstname"/>
  </parameters>
  <parameters predicate="queried_phonenumber">
    <background predicate="queried_contact"/>
  </parameters>
</domain>
