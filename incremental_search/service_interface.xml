<?xml version="1.0" encoding="utf-8"?>
<service_interface>
  <action name="Call">
    <parameters>
      <parameter predicate="selected_contact" format="value"/>
    </parameters>
    <failure_reasons/>
    <target>
      <device_module device="IncrementalSearchDevice"/>
    </target>
  </action>
  <action name="DeleteNumber">
    <parameters>
      <parameter predicate="selected_contact" format="value"/>
    </parameters>
    <failure_reasons/>
    <target>
      <device_module device="IncrementalSearchDevice"/>
    </target>
  </action>
  <query name="selected_contact">
    <parameters>
      <parameter predicate="selected_first_name" format="value" optional="true"/>
      <parameter predicate="selected_last_name" format="value" optional="true"/>
    </parameters>
    <target>
      <device_module device="IncrementalSearchDevice"/>
    </target>
  </query>
  <entity_recognizer name="ContactRecognizer">
    <target>
      <device_module device="IncrementalSearchDevice"/>
    </target>
  </entity_recognizer>
  <entity_recognizer name="NumberTypeRecognizer">
    <target>
      <device_module device="IncrementalSearchDevice"/>
    </target>
  </entity_recognizer>
</service_interface>